{
  "name": "gugler/ckeditor-language",
  "type": "typo3-cms-extension",
  "description": "The text part language feature lets you mark the language of text fragments. This way browsers and screen readers can correctly interpret parts written in different languages.",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "ckeditor",
    "typo3",
    "plugin",
    "editor",
    "language"
  ],
  "homepage": "https://www.markensinn.at",
  "authors": [
    {
      "name": "Markus Ketterer",
      "email": "webdeveloper@gugler.at",
      "homepage": "https://www.markensinn.at",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-rte-ckeditor": "^12.4 || ^13.4"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "ckeditor_language"
    }
  }
}
